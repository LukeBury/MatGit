function [dX] = Int_CR3Bn_ZH_new(t,X,prms)
%%% For numerical integration in the normalized CR3BP with J2 of each body
%%% Inputs:
%          t - normalized time vector
%          X - initial state [6x1]
%          prms - (u, R1_n, R2_n, J2p, J2s, J3p, J3s, J4p, J4s)
%
%
%          u - mass ratio of CR3BP system
%          R1 - radius of primary body
%          R2 - radius of secondary body
%          J2p - J2 of primary body
%          J2s - J2 of secondary body
%          J3p - J3 of primary body
%          J3s - J3 of secondary body
%          J4p - J4 of primary body
%          J4s - J4 of secondary body
% =======================================================================
%%% Unpacking some parameters (for brevity/clarity)
mu = prms.u;

%%% Distances to primary (1) and secondary (2) bodies
r1 = sqrt((X(1)+mu)^2 + X(2)^2 + X(3)^2);
r2 = sqrt((X(1)+mu-1)^2 + X(2)^2 + X(3)^2);

if isfield(prms, 'J2p')
%     gamma_J2p = 3*(1-u)*(prms.R1^2)*prms.J2p*(5*X(3)^2-r1^2)*(x+u) / (2*r1^7);
    gamma_J2p = 3*(1-mu)*(prms.R1^2)*prms.J2p*(5*X(3)^2-r1^2) / (2*r1^7);
    az_J2p    = 3*(1-mu)*(prms.R1^2)*prms.J2p*(5*X(3)^2-3*r1^2)*X(3) / (2*r1^7);
else
    gamma_J2p = 0;
    az_J2p    = 0;
end

if isfield(prms, 'J2s')
    % %%% From new derivation (General form)
    % % dMdx_J2 = 3*GM*R^2*prms.J2*(5*X(3)^2-r^2)*x / (2*r^7);
    % % dMdz_J2 = 3*GM*R^2*prms.J2*(5*X(3)^2-3*r^2)*X(3) / (2*r^7);
    gamma_J2s = 3*mu*prms.R2^2*prms.J2s*(5*X(3)^2-r2^2) / (2*r2^7);
    az_J2s    = 3*mu*prms.R2^2*prms.J2s*(5*X(3)^2-3*r2^2)*X(3) / (2*r2^7);
else
    gamma_J2s = 0;
    az_J2s    = 0;
end

if isfield(prms, 'J3p')
%     gamma_J3p = 5*(1-u)*(prms.R1^3)*J3p*X(3)*(7*X(3)^2 - 3*r1^2) * (x+u) / (2*r1^9);
    gamma_J3p = 5*(1-mu)*(prms.R1^3)*J3p*X(3)*(7*X(3)^2 - 3*r1^2) / (2*r1^9);
    az_J3p    = (1-mu)*(prms.R1^3)*J3p*(3*r1^4 - 30*X(3)^2*r1^2 + 35*X(3)^4) / (2*r1^9);
else
    gamma_J3p = 0;
    az_J3p    = 0;
end

if isfield(prms, 'J3s')
    % %%% From new derivation (General form)
    % % dMdx_J3 = 5*GM*(R^3)*J3*X(3)*(6*X(3)^2 - 3*r^2) * x / (2*r^9);
    % % dMdz_J3 = GM*(R^3)*J3*(3*r^4 - 30*X(3)^2*r^2 + 35*X(3)^4) / (2*r^9);
    gamma_J3s = 5*mu*(prms.R2^3)*J3s*X(3)*(6*X(3)^2 - 3*r2^2) / (2*r2^9);
    az_J3s    = mu*(prms.R2^3)*J3s*(3*r2^4 - 30*X(3)^2*r2^2 + 35*X(3)^4) / (2*r2^9);
else
    gamma_J3s = 0;
    az_J3s    = 0;
end

if isfield(prms, 'J4p')
%     gamma_J4p = 15*(1-u)*(prms.R1^4)*J4p*(21*X(3)^4 - 14*r1^2*X(3)^2 + r1^4) * (x+u) / (8*r1^11);
    gamma_J4p = 15*(1-mu)*(prms.R1^4)*J4p*(21*X(3)^4 - 14*r1^2*X(3)^2 + r1^4) / (8*r1^11);
    az_J4p    = 5*(1-mu)*(prms.R1^4)*J4p*X(3)*(15*r1^4 - 70*X(3)^2*r1^2 + 63*X(3)^4) / (8*r1^11);
else
    gamma_J4p = 0;
    az_J4p    = 0;
end

if isfield(prms, 'J4s')
    % %%% From new derivation (General form)
    % % dMdx_J4 = 15*GM*(R^4)*J4*(21*X(3)^4 - 14*r^2*X(3)^2 + r^4) * x / (8*r^11);
    % % dMdz_J4 = 5*GM*(R^4)*J4*X(3)*(15*r^4 - 70*X(3)^2*r^2 + 63*X(3)^4) / (8*r^11);
    gamma_J4s = 15*mu*(prms.R2^4)*J4s*(21*X(3)^4 - 14*r2^2*X(3)^2 + r2^4) * x / (8*r2^11);
    az_J4s    = 5*mu*(prms.R2^4)*J4s*X(3)*(15*r2^4 - 70*X(3)^2*r2^2 + 63*X(3)^4) / (8*r2^11);
else
    gamma_J4s = 0;
    az_J4s    = 0;
end

if isfield(prms, 'J5p')
%     gamma_J5p = 7*(1-u)*(prms.R1^5)*J5p*X(3)*(15*r1^4 - 90*X(3)^2*r1^2 + 99*X(3)^4) * (x+u) / (8*r1^13);
    gamma_J5p = 7*(1-mu)*(prms.R1^5)*J5p*X(3)*(15*r1^4 - 90*X(3)^2*r1^2 + 99*X(3)^4) / (8*r1^13);
    az_J5p    = 3*(1-mu)*(prms.R1^5)*J5p*(-5*r1^6 + 105*X(3)^2*r1^4 - 315*X(3)^4*r1^2 + 231*X(3)^6) / (8*r1^13);
else
    gamma_J5p = 0;
    az_J5p    = 0;
end

if isfield(prms, 'J5s')
    % %%% From new derivation (General form)
    % % dMdx_J5 = 7*GM*(R^5)*J5*X(3)*(15*r^4 - 90*X(3)^2*r^2 + 99*X(3)^4) * x / (8*r^13);
    % % dMdz_J5 = 3*GM*(R^5)*J5*(-5*r^6 + 105*X(3)^2*r^4 - 315*X(3)^4*r^2 + 231*X(3)^6) / (8*r^13);
    gamma_J5s = 7*mu*(prms.R2^5)*J5s*X(3)*(15*r2^4 - 90*X(3)^2*r2^2 + 99*X(3)^4) / (8*r2^13);
    az_J5s    = 3*mu*(prms.R2^5)*J5s*(-5*r2^6 + 105*X(3)^2*r2^4 - 315*X(3)^4*r2^2 + 231*X(3)^6) / (8*r2^13);
else
    gamma_J5s = 0;
    az_J5s    = 0;
end

if isfield(prms, 'J6p')
%     gamma_J6p = 7*(1-u)*(prms.R1^6)*J6p*(-5*r1^6 + 135*X(3)^2*r1^4 - 495*X(3)^4*r1^2 + 429*X(3)^6) * (x+u) / (16*r1^15);
    gamma_J6p = 7*(1-mu)*(prms.R1^6)*J6p*(-5*r1^6 + 135*X(3)^2*r1^4 - 495*X(3)^4*r1^2 + 429*X(3)^6) / (16*r1^15);
    az_J6p    = 7*(1-mu)*(prms.R1^6)*J6p*X(3)*(-35*r1^6 + 315*X(3)^2*r1^4 - 693*X(3)^4*r1^2 + 429*X(3)^6) / (16*r1^15);
else
    gamma_J6p = 0;
    az_J6p    = 0;
end

if isfield(prms, 'J6s')
    % %%% From new derivation (General form)
    % % dMdx_J6 = 7*GM*(R^6)*J6*(-5*r^6 + 135*X(3)^2*r^4 - 495*X(3)^4*r^2 + 429*X(3)^6) * x / (16*r^15);
    % % dMdz_J6 = 7*GM*(R^6)*J6*X(3)*(-35*r^6 + 315*X(3)^2*r^4 - 693*X(3)^4*r^2 + 429*X(3)^6) / (16*r^15);
    gamma_J6s = 7*mu*(prms.R2^6)*J6s*(-5*r2^6 + 135*X(3)^2*r2^4 - 495*X(3)^4*r2^2 + 429*X(3)^6) / (16*r2^15);
    az_J6s    = 7*mu*(prms.R2^6)*J6s*X(3)*(-35*r2^6 + 315*X(3)^2*r2^4 - 693*X(3)^4*r2^2 + 429*X(3)^6) / (16*r2^15);
else
    gamma_J6s = 0;
    az_J6s    = 0;
end

%%% Equations of Motion
ddx = 2*X(5) + X(1) + (-(1-mu)/r1^3 + gamma_J2p + gamma_J3p + gamma_J4p + gamma_J5p + gamma_J6p)*(X(1) + mu) + (-mu/r2^3 + gamma_J2s + gamma_J3s + gamma_J4s + gamma_J5s + gamma_J6s)*(X(1) - 1 + mu);
ddy = -2*X(4) + X(2) + X(2)*(-(1-mu)/r1^3 - mu/r2^3 + gamma_J2p + gamma_J2s + gamma_J3p + gamma_J3s + gamma_J4p + gamma_J4s + gamma_J5p + gamma_J5s + gamma_J6p + gamma_J6s);
ddz = X(3)*(-(1-mu)/r1^3 - mu/r2^3) + az_J2p + az_J2s + az_J3p + az_J3s + az_J4p + az_J4s + az_J5p + az_J5s + az_J6p + az_J6s;

%%% Output the derivative of the state
dX(1:3) = [X(4); X(5); X(6)]; 
dX(4:6) = [ddx; ddy; ddz]; 








end
